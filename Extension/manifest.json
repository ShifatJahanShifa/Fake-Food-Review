{
    "manifest_version": 3,
    "name": "Fake Food Review Detection Extension",
    "description": "A Chrome extension that predicts whether food reviews are likely to be Fake or Real",
    "version": "1.0",
    "icons": {
      "16": "images/image.png",
      "48": "images/image.png",
      "128": "images/image.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://127.0.0.1:5000"
    ],
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "resources": ["popup.js"],
        "matches": ["http://*/*", "https://*/*"]
      }
    ]
  }